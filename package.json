{
  "name": "unplugin-strive-molu",
  "packageManager": "pnpm@8.15.5",
  "version": "0.0.1",
  "description": "在strive-molu使用组件时自动导入样式",
  "keywords": [
    "strive-molu",
    "unplugin",
    "vite"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.ts",
        "import": "./dist/index.d.mts"
      },
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": {
        "require": "./dist/vite.d.ts",
        "import": "./dist/vite.d.mts"
      },
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhoubaos/unplugin-strive-molu"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "zx:tag": "tsx scripts/zx/tag.ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "vite:dev": "pnpm -C example/vite run dev",
    "vite:build": "pnpm -C example/vite run build"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "es-module-lexer": "^1.5.0",
    "magic-string": "^0.30.10",
    "unplugin": "^1.10.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.13",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "chalk": "^5.3.0",
    "change-case": "^5.4.4",
    "consola": "^3.2.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "fast-glob": "^3.3.2",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.11.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "yaml-eslint-parser": "^1.2.3",
    "zx": "^8.1.2"
  },
  "engines": {
    "node": ">=16.20.2"
  }
}
